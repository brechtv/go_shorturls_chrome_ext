{
  "description": "#ShortURLs for everyone",
  "manifest_version": 2,
  "name": "#goslash",
  "version": "1.0",
   "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}